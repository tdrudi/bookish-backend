{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/userAuth.json",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer"
        },
        "user_id": {
            "type": "integer"
        },
        "book_id": {
            "type": "integer"
        },
        "rating": {
            "type": "integer",
            "minimum": 1,
            "maximum": 5
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "user_id",
        "book_id",
        "rating"
    ],
    "additionalProperties": false
}